{
	"name": "setlist-generator",
	"version": "0.1.0",
	"private": true,
	"license": "UNLICENSED",
	"main": "public/electron.js",
	"homepage": "./",
	"author": {
		"name": "geeves"
	},
	"description": "setlist generator for fun because I can, not because I should.",
	"scripts": {
		"magic": "webpack",
		"start": "react-scripts start",
		"build": "react-scripts build",
		"test": "react-scripts test",
		"eject": "react-scripts eject",
		"dev": "concurrently -k \"BROWSER=none yarn start\" \"yarn:electron\"",
		"electron": "wait-on tcp:3000 && electron .",
		"package": "react-scripts build && electron-forge package",
		"make-mac": "react-scripts build && electron-forge make --platform darwin",
		"make-linux": "react-scripts build && electron-forge make --platform linux",
		"make": "react-scripts build && electron-forge make",
		"prepare": "husky install"
	},
	"eslintConfig": {
		"extends": [
			"react-app",
			"react-app/jest"
		]
	},
	"browserslist": {
		"production": [
			">0.2%",
			"not dead",
			"not op_mini all"
		],
		"development": [
			"last 1 chrome version",
			"last 1 firefox version",
			"last 1 safari version"
		]
	},
	"dependencies": {
		"@commitlint/cli": "^17.0.3",
		"@commitlint/config-conventional": "^17.0.3",
		"@emotion/react": "^11.10.0",
		"@emotion/styled": "^11.10.0",
		"@mui/material": "^5.10.2",
		"@reduxjs/toolkit": "^1.8.5",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.3.0",
		"@testing-library/user-event": "^14.4.3",
		"@types/jest": "^28.1.8",
		"@types/node": "^18.7.13",
		"@types/react": "^18.0.17",
		"@types/react-dom": "^18.0.6",
		"axios": "^0.27.2",
		"electron-is-dev": "^2.0.0",
		"electron-squirrel-startup": "^1.0.0",
		"react": "^18.2.0",
		"react-dom": "^18.2.0",
		"react-redux": "^8.0.2",
		"react-scripts": "^5.0.1",
		"swr": "^1.3.0",
		"ts-loader": "^9.3.1",
		"typescript": "^4.8.2",
		"web-vitals": "^3.0.0",
		"webpack": "^5.74.0",
		"webpack-cli": "^4.10.0"
	},
	"devDependencies": {
		"@electron-forge/cli": "^6.0.0-beta.65",
		"@electron-forge/maker-deb": "^6.0.0-beta.65",
		"@electron-forge/maker-dmg": "^6.0.0-beta.65",
		"@electron-forge/maker-rpm": "^6.0.0-beta.65",
		"@electron-forge/maker-squirrel": "^6.0.0-beta.65",
		"@electron-forge/maker-zip": "^6.0.0-beta.65",
		"@google/semantic-release-replace-plugin": "1.1.0",
		"@semantic-release/changelog": "6.0.1",
		"@semantic-release/exec": "6.0.3",
		"@semantic-release/git": "10.0.1",
		"concurrently": "^7.2.2",
		"conventional-changelog-conventionalcommits": "5.0.0",
		"electron": "^20.1.0",
		"gradle-semantic-release-plugin": "1.7.3",
		"lint-staged": "^13.0.3",
		"prettier": "2.7.1",
		"wait-on": "^6.0.1"
	},
	"config": {
		"forge": {
			"packagerConfig": {},
			"makers": [
				{
					"name": "@electron-forge/maker-squirrel",
					"config": {
						"name": "stock_trading_app"
					}
				},
				{
					"name": "@electron-forge/maker-deb",
					"config": {
						"name": "stock_trading_app"
					}
				},
				{
					"name": "@electron-forge/maker-rpm",
					"config": {
						"name": "stock_trading_app"
					}
				},
				{
					"name": "@electron-forge/maker-dmg",
					"config": {
						"name": "stock_trading_app",
						"format": "ULFO"
					}
				},
				{
					"name": "@electron-forge/maker-zip",
					"platforms": [
						"darwin",
						"linux",
						"win32"
					]
				}
			]
		}
	},
	"lint-staged": {
		"**/*": "prettier --write --ignore-unknown"
	}
}
